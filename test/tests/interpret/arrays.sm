program {
    array x[0:4];
    array y[true ? -3 : 1:-1];
    var a = 2;
    array z[a * a + 1:4 + 3];
    var i = 4;
    func f(a) {
        a
    }

    x[0] = 5;
    x[1] = 4;
    x[2] = 3;
    x[3] = 2;
    x[4] = 1;

    output x[0]; // 5
    output x[1]; // 4
    output x[2]; // 3
    output x[3]; // 2
    output x[4]; // 1

    while (i >= 0) {
        output x[i];
        i = i - 1;
    } // 5\n4\n3\n2\n1

    y[-3] = "this ";
    y[-2] = "is a ";
    y[-1] = "test";
    output y[-3] + y[-2] + y[-1]; // this is a test

    z[5] = 1.25;
    z[6] = true;
    z[7] = 1.5;

    output z[6] ? z[5] : z[7]; // 1.25
    z[6] = 5;
    output (true ? z : y)[6] < 2 ? z[5] : z[7]; // 1.5

    {
        proc update(xs, n, v) {
            xs[n] = v;
        }

        update(z, 5, "test");
        output z[5]; // test
    }

    output f(x)[f(0)]; // 5
}