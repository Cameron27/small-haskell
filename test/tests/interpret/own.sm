program {
    proc incOut() {
        own x = 0;
        own x = 1;
        output x;
        x = x + 1;
    }
    rec proc outX(x) {
        own i = 1;
        if (i <= x) {
            output i;
            i = i + 1;
            outX(x);
        }
    }
    var t = false;

    incOut();
    incOut();

    repeat {
        own x = true ? 1 : 3;
        output x;
        x = x + 1;
        t = x == 5;
    } until (t)

    incOut();
    incOut();

    outX(4);
}